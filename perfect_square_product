import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int m = sc.nextInt();
        int prod = n * m;
        int root = (int)Math.sqrt(prod);
        if(root * root == prod){
            System.out.print("yes");
        } else {
            System.out.print("no");
        }
    }
}
