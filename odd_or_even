import java.util.*;
public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        double num = sc.nextDouble();   
        long rounded = Math.round(num);
        if (rounded == 0) {
            System.out.println("Zero");
        }
        else if (rounded % 2 == 0) {
            System.out.println("Even");
        }
        else {
            System.out.println("Odd");
        }
    }
}
