import java.util.*;
class Main {
    public static void main(String[] args){
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int sum=0;
        System.out.print("The Sum of natural numbers are: ");
        for(int i=0;i<=n;i++){
            sum+=i;
        }
        System.out.print(sum);
    }
}

